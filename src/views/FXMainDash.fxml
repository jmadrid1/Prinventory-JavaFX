<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="290.0" prefWidth="576.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FXMainDashController">
   <children>
      <ImageView fitHeight="287.0" fitWidth="600.0">
         <image>
            <Image url="@../resources/main_dash.png" />
         </image>
      </ImageView>
      <ImageView fx:id="printerDashBtn" fitHeight="101.0" fitWidth="104.0" layoutX="46.0" layoutY="122.0" onMouseClicked="#switchToPrinterDash">
         <Image url="@../resources/ic_printer.png" />
      </ImageView>
      <ImageView fx:id="tonerDashBtn" fitHeight="101.0" fitWidth="104.0" layoutX="248.0" layoutY="125.0" onMouseClicked="#switchToTonerDash">
         <Image url="@../resources/ic_toner.png" />
      </ImageView>
      <ImageView fx:id="vendorDashBtn" fitHeight="101.0" fitWidth="104.0" layoutX="447.0" layoutY="124.0" onMouseClicked="#switchToVendorDash">
         <Image url="@../resources/ic_vendor.png" />
      </ImageView>
      <Text layoutX="84.0" layoutY="259.0" opacity="0.71" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Select The Desired Dashboard You Would Like To Navigate To">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <ImageView fx:id="closeWindowBtn" fitHeight="19.0" fitWidth="13.0" layoutX="577.0" layoutY="4.0" opacity="0.41" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="4">
         <image>
            <Image url="@../resources/ic_close_window.png" />
         </image>
      </ImageView>
      <ImageView fx:id="minimizeWindowBtn" fitHeight="13.0" fitWidth="13.0" layoutX="557.0" layoutY="5.0" opacity="0.43" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="20">
         <image>
            <Image url="@../resources/ic_minimize_window.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
